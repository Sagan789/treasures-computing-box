{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.0.8",
        "components/jquery": "3.*",
        "components/jqueryui": "^1.12",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/asset": "^3.4",
        "symfony/console": "^3.4",
        "symfony/flex": "^1.0",
        "symfony/form": "^3.4",
        "symfony/framework-bundle": "^3.4",
        "symfony/lts": "^3",
        "symfony/profiler-pack": "^1.0",
        "symfony/twig-bundle": "^3.4",
        "symfony/validator": "^3.4",
        "symfony/yaml": "^3.4",
        "twbs/bootstrap": "^3.3",
        "twig/twig": "^1.0||^2.0"
    },
    "require-dev": {
        "symfony/dotenv": "^3.4",
        "symfony/phpunit-bridge": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts",
            "cp vendor/twbs/bootstrap/dist/css/bootstrap.css public/css/",
            "cp vendor/twbs/bootstrap/dist/fonts/* public/fonts/",
            "cp vendor/twbs/bootstrap/dist/js/bootstrap.js public/js/",
            "cp vendor/components/jquery/jquery.min.js public/js/",
            "cp vendor/components/jqueryui/jquery-ui.min.js public/js/"
        ],
        "post-update-cmd": [
            "@auto-scripts",
            "cp vendor/twbs/bootstrap/dist/css/bootstrap.css public/css/",
            "cp vendor/twbs/bootstrap/dist/fonts/* public/fonts/",
            "cp vendor/twbs/bootstrap/dist/js/bootstrap.js public/js/",
            "cp vendor/components/jquery/jquery.min.js public/js/",
            "cp vendor/components/jqueryui/jquery-ui.min.js public/js/"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C0EKF1P12THZCR8NAGH5EVH2",
            "allow-contrib": false
        }
    }
}
